local kavoUi = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local window = kavoUi.CreateLib("Dawidka Hub","Serpent")

--- tabs
local Tab1 = window:NewTab("Other")
local Tab1Section = Tab1:NewSection("Other Scripts")

local Tab2 = window:NewTab("Murder Mystery")
local Tab2Section = Tab2:NewSection("MM2 Scripts")

local Tab4 = window:NewTab("Pet Simulator X")
local Tab4Section = Tab4:NewSection("PSX Scripts")

local Tab5 = window:NewTab("Mining Simulator 2")
local Tab5Section = Tab5:NewSection("Mining Simulator 2 Scripts")

local Tab6 = window:NewTab("Sword Fighters")
local Tab6Section = Tab6:NewSection("Sword Fighters Scripts")

local Tab3 = window:NewTab("Info")
local Tab3Section = Tab3:NewSection("Made by Dawidka")


--- Buttons

Tab1Section:NewButton("Infinity Yield","Admin Commands",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

Tab1Section:NewButton("KaiZen Game Hub","Opens KaiZen Game Hub",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Kaizenofficiall/KaiZen/main/GameHub", true))()
    end)

Tab2Section:NewButton("HOHO Hub","Opens HOHO Hub script", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI'))()
    end)

Tab2Section:NewButton("MM2 Esp","Shows everyone's position and their roles in Murder Mystery2",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Ihaveash0rtnamefordiscord/Releases/main/MurderMystery2HighlightESP"))()
end)

Tab4Section:NewButton("PSX Script Menu","Script menu with Pet Simulator scripts [use jmes gui on phone]",function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/UltraStuff/scripts2/main/petsimx", true))()
end)

    Tab4Section:NewButton("Project WD","Project WD menu for Pet Simulator X",function()
        getgenv().key = "projectWBIsAwesomemrcrapcrappypattt"
loadstring(game:HttpGet("https://raw.githubusercontent.com/Muhammad6196/Project-WD/main/Main.lua"))()
end)

Tab5Section:NewButton("Project WD","Project WD menu for Mining Simulator 2",function()
