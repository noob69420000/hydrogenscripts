Tab6Section:NewButton("SCRIPT #1","",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/SpaceCat1748/Boblox/main/SFS.lua"))()
end)

local Tab6Section2 = Tab6:NewSection("Eggs")
Tab6Section2:NewButton("Open Lava Egg","",function()
local args = {
    [1] = {
        ["eggName"] = "Egg 11",
        ["baseAmount"] = 1,
        ["auto"] = false,
        ["amount"] = 4
    }
}

game:GetService("ReplicatedStorage").Packages.Knit.Services.EggService.RF.BuyEgg:InvokeServer(unpack(args))
end)
